intModuleNum = 66;


void progCB(DB x) 
{ 
	
	string filename= "c:\\baseline_list.csv"
	Stream stream_file= write filename;
	stream_file << "Module,Baseline,Created By,Created On\n"
	
	int step= 0;
	int i= 0;
	progressStart(x, "Processing", "", intModuleNum);
	
	for(i=0; i<intModuleNum; i++)
	{
		progressStep ++step;
	
		Module mod= read(strArrModule[i], false);
	
		if(mod!=null)
		{
			string strModule = name mod;
			Baseline bl= getMostRecentBaseline(mod) 
		
			stream_file << strArrModule[i] ","  (major bl)"."(minor bl) "," (user bl) "," (dateOf bl) "\n" 

			close(mod);
		}
		else
		{
			print "Can not find--["(strArrModule[i])"]\n"
		}
	}
	progressStop();
}




DB progressDB = create "Progress Baseline" 
label(progressDB, "Get Baseline") 
apply(progressDB, progCB) 
show progressDB 
 